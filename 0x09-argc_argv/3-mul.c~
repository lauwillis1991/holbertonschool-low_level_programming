#include "holberton.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * main - multiplies two numbers
 * @argc: void
 * @argv: argument vector will be used.
 * Return: 0 unless error which will be 1
 */

int main(int argc, char *argv[])
{
	(void) argc;


	if (argv[1] && argv[2])
	{

		printf("%d\n", atoi(argv[1]) * atoi(argv[1]));
		return (0);
	}

	else
	{
		printf("Error\n");
		return (1);
	}

}
